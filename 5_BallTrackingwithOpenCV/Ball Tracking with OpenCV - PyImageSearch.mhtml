@charset "utf-8";

html { overflow: hidden; }

body { font: 12px Roboto, Arial, sans-serif; background-color: rgb(0, 0, 0); color: rgb(255, 255, 255); height: 100%; width: 100%; overflow: hidden; position: absolute; margin: 0px; padding: 0px; }

#player { width: 100%; height: 100%; }

h1 { text-align: center; color: rgb(255, 255, 255); }

h3 { margin-top: 6px; margin-bottom: 3px; }

.player-unavailable { position: absolute; top: 0px; left: 0px; right: 0px; bottom: 0px; padding: 25px; font-size: 13px; background: url("/img/meh7.png") 50% 65% no-repeat; }

.player-unavailable .message { text-align: left; margin: 0px -5px 15px; padding: 0px 5px 14px; border-bottom: 1px solid rgb(136, 136, 136); font-size: 19px; font-weight: normal; }

.player-unavailable a { color: rgb(22, 122, 198); text-decoration: none; }

.exp-invert-logo #header::before, .exp-invert-logo .ypc-join-family-header .logo, .exp-invert-logo #footer-logo .footer-logo-icon, .exp-invert-logo #yt-masthead #logo-container .logo, .exp-invert-logo #masthead #logo-container, .exp-invert-logo .admin-masthead-logo a, .exp-invert-logo #yt-sidebar-styleguide-logo #logo { background: url("/yts/img/ringo/hitchhiker/logo_small_2x-vfl4_cFqn.png") 0% 0% / 100px 30px no-repeat; }

.exp-invert-logo #yt-masthead #logo-container .logo-red { background: url("/yts/img/ringo/hitchhiker/logo_youtube_red_2x-vflOSHA_n.png") 0% 0% / 132px 30px no-repeat; }

@media only screen and (max-width: 498px) and (min-width: 0px), only screen and (max-width: 704px) and (min-width: 499px) {
  .exp-invert-logo.exp-responsive #yt-masthead #logo-container { background: url("/yts/img/ringo/hitchhiker/yt_play_logo_2x-vflXx5Pg3.png") 0% 0% / 40px 28px no-repeat; }
}

